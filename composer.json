{
    "name": "libero/dummy-api",
    "description": "Dummy implementation of a Libero API for development and testing purposes.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Libero\\DummyApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\Libero\\DummyApi\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "libero/api-problem-bundle": "^0.1",
        "libero/content-api-bundle": "^0.1",
        "libero/ping-controller": "^0.1",
        "symfony/config": "^4.1",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/filesystem": "^4.1",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/http-foundation": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/monolog-bundle": "^3.3",
        "symfony/routing": "^4.1",
        "symfony/translation": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "fluentdom/fluentdom": "^7.0",
        "libero/coding-standard": "^0.3",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpstan/phpstan-symfony": "^0.10",
        "phpunit/phpunit": "^7.4",
        "symfony/debug": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/web-profiler-bundle": "^4.1",
        "thomasweinert/phpunit-xpath-assertions": "^2.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
